{
    "name": "Zlib Decompressor",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": [
          "content.js"
        ]
      }
    ]
  }